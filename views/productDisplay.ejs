<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    
    <h1>New Product</h1>
    <% if (typeof products != 'undefined' && products.length >= 0) { %>
        <% products.forEach(product => { %>

            <div id="product-type">
                <h1> <%= product.name %> </h1>
                <p> <%= product.price %> </p>
                <p> <%= product.type %> </p>
                <p> <%= product.company %> </p>
            </div>

        <% }) %>
    <% } %>
   
</body>
</html>